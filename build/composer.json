{
  "authors": [
    {
      "name": "UNB Libraries libsupport@unb.ca",
      "role": "Creator"
    }
  ],
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "drupal/core-composer-scaffold": true,
      "phpstan/extension-installer": true,
      "oomphinc/composer-installers-extender": true
    },
    "discard-changes": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "description": "Drupal 8 component of nbbib.lib.unb.ca",
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "./"
      }
    },
    "enable-patching": true,
    "installer-paths": {
      "core": [
        "type:drupal-core"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "libraries/{$name}": [
        "type:drupal-library"
      ],
      "modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "Commands/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "profiles/custom/{$name}": [
        "type:drupal-custom-profile"
      ],
      "themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ]
    },
    "patches": {
      "drupal/core": {
        "Fix error on entity reference creation with parentheses in name": "https://www.drupal.org/files/issues/2021-04-18/2924783-18.patch"
      }
    }
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "name": "unb-libraries/nbbib.lib.unb.ca",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.7/chosen_v1.8.7.zip"
        },
        "name": "harvesthq/chosen",
        "type": "drupal-library",
        "version": "1.8.7"
      },
      "type": "package"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/furf/jquery-ui-touch-punch/archive/refs/heads/master.zip"
        },
        "name": "furf/jquery-ui-touch-punch",
        "type": "drupal-library",
        "version": "dev-master"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/simeydotme/jQuery-ui-Slider-Pips/archive/v1.11.4.zip"
        },
        "name": "simeydotme/jquery-ui-slider-pips",
        "require": {
          "composer/installers": "^2"
        },
        "type": "drupal-library",
        "version": "1.11.4"
      },
      "type": "package"
    },
    {
      "package": {
        "name": "drupal/login_return_page",
        "source": {
          "reference": "8.x-1.x",
          "type": "git",
          "url": "https://git.drupalcode.org/sandbox/gnuget-2824414.git"
        },
        "type": "drupal-module",
        "version": "1.x-dev"
      },
    "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/OwlCarousel2/OwlCarousel2/archive/2.3.4.zip"
        },
        "name": "owlcarousel2/owlcarousel2",
        "type": "drupal-library",
        "version": "2.3.4"
      },
      "type": "package"
    }
  ],
  "require": {
    "drupal/admin_toolbar": "^3",
    "drupal/autocomplete_deluxe": "^2",
    "drupal/block_content_permissions": "^1",
    "drupal/bootstrap4": "^3",
    "drupal/bootstrap5": "^3",
    "drupal/color": "^1",
    "drupal/core": "^10",
    "drupal/core-composer-scaffold": "^10",
    "drupal/core-recommended": "^10",
    "drupal/easy_breadcrumb": "^2",
    "drupal/facets": "^2",
    "drupal/fontawesome": "^2",
    "drupal/google_analytics": "^4",
    "drupal/health_check": "^1",
    "drupal/inline_entity_form": "^1",
    "drupal/jquery_ui": "^1",
    "drupal/jquery_ui_slider": "^2",
    "drupal/jquery_ui_touch_punch": "^1",
    "drupal/paragraphs": "^1",
    "drupal/pathauto": "^1",
    "drupal/quickedit": "^1",
    "drupal/r4032login": "^2", 
    "drupal/redirect": "^1",
    "drupal/redis": "^1",
    "drupal/samlauth": "^3",
    "drupal/search_api": "1.34",
    "drupal/search_api_autocomplete": "^1",
    "drupal/search_api_solr": "^4",
    "drupal/token": "^1",
    "drupal/twig_field_value": "^2",
    "drupal/twig_tweak": "^3",
    "drupal/views_contextual_filters_or": "^1",
    "furf/jquery-ui-touch-punch": "dev-master",
    "harvesthq/chosen": "^1",
    "oomphinc/composer-installers-extender": "^2",
    "owlcarousel2/owlcarousel2": "2.3.4",
    "pear/file_marc": "^1",
    "scriptotek/marc": "^3",
    "simeydotme/jquery-ui-slider-pips": "^1",
    "unb-libraries/composer-drupal-scaffold": "^10",
    "unb-libraries/saml_features": "dev-10.x-1.x"
  },
  "require-dev": {
    "unb-libraries/drupal-development-libraries": "^10"
  },
  "scripts": {
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "pre-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "pre-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ]
  },
  "type": "project"
}
