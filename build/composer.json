{
  "authors": [
    {
      "name": "UNB Libraries libsupport@unb.ca",
      "role": "Creator"
    }
  ],
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "drupal/core-composer-scaffold": true
    },
    "discard-changes": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "description": "Drupal 8 component of nbbib.lib.unb.ca",
  "extra": {
    "enable-patching": true,
    "installer-paths": {
      "core": [
        "type:drupal-core"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "libraries/{$name}": [
        "type:drupal-library"
      ],
      "modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "themes/contrib/{$name}": [
        "type:drupal-theme"
      ]
    },
    "patches": {
      "drupal/samlauth": {
        "Ignore Role Restrictions": "/build/patches/samlauth_rolematch.patch"
      }
    }
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "name": "unb-libraries/nbbib.lib.unb.ca",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.7/chosen_v1.8.7.zip"
        },
        "name": "harvesthq/chosen",
        "type": "drupal-library",
        "version": "1.8.7"
      },
      "type": "package"
    }
  ],
  "require": {
    "drupal/admin_toolbar": "3.1.1",
    "drupal/bootstrap4": "2.1.15",
    "drupal/chosen": "3.0.3",
    "drupal/core": "9.4.5",
    "drupal/externalauth": "1.4",
    "drupal/health_check": "1.3",
    "drupal/inline_entity_form": "1.x-dev",
    "drupal/paragraphs": "1.15",
    "drupal/pathauto": "1.11",
    "drupal/redirect": "1.7",
    "drupal/redis": "1.5",
    "drupal/samlauth": "3.8",
    "drupal/search_api": "1.25",
    "drupal/search_api_autocomplete": "1.6",
    "drupal/search_api_solr": "4.2.8",
    "drupal/token": "1.11",
    "drupal/transliterate": "1.4",
    "drupal/twig_field_value": "2.0.0",
    "drupal/twig_tweak": "2.9",
    "drupal/views_contextual_filters_or": "1.3",
    "harvesthq/chosen": "1.8.7",
    "unb-libraries/drupal-builder": "^9",
    "unb-libraries/saml_features": "dev-9.x-1.x"
  },
  "require-dev": {
    "unb-libraries/drupal-development-libraries": "^9"
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "pre-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "pre-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ]
  },
  "type": "project"
}
