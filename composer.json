{
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "name": "unb-libraries/nbbib.lib.unb.ca",
  "description": "Scaffold and provision local development for nbbib.lib.unb.ca",
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "require": {
    "unb-libraries/unb_lib_theme": "dev-9.x-bootstrap-3.x"
  },
  "require-dev": {
    "unb-libraries/dockworker-drupal": "^5"
  },
  "scripts": {
    "post-update-cmd": [
      "vendor/bin/dockworker dockworker:git:setup-hooks"
    ]
  }
}
